.who-we-are
  background #000

  .video-wrapper
    position relative
    margin 0 auto
    overflow hidden
    width 100%
    max-height 500px
    max-height calc(80vh - 60px)

    &:before
      content ''
      display block
      padding-top 56%

    iframe
      position absolute
      left 0
      top 0
      width 100%
      height 100%
